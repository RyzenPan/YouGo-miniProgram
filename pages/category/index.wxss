.left .cate-btn {
  line-height: 70rpx;
  text-align: center;
  background-color: #f8f8f8;
  border: 1px solid #fff;
  font-size: 28rpx;
  color: #333;
}

.catePage {
  display: flex;
}

.right .title {
  text-align: center;
  font-size: 32rpx;
  padding: 16rpx 0;
}

.right .main {
  padding: 40rpx 5rpx 40rpx 25rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: start;
}

.right .main-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  flex: 0 0 33.33%;
  font-size: 12px;
}

.right .main-item image{
  width: 120rpx;
  height: 120rpx;
  margin-bottom: 10rpx;
}


.leftStyle{
  width: 20%;
  height: 100%;
  position: fixed; 
}

.rightStyle{
  padding-left: 180rpx;
}

.active {
  color: red;
}